// Example program
#include <iostream>
#include <string>
using namespace std;
int fukcja(int z, int k);
int main()
{
int a = 49;
int b = 6;

//cout<<"losowane liczby" << endl;


int k = fukcja(a,b);
cout<<k;

return 0;
}


int fukcja(int z , int k)
{
   long int los = z ;// 10
   long int los1 = k; //4
   long int temp = 1;
   long int dzielenie = los - los1;
    long int suma = 1;
   long int odejmowanie = 1 ;

for (int i = 1; i <= los; i++) // silnia 
{
        suma *= i;
}

for (int i = 1; i <= los1; i++) // silnia 
{
        temp *= i;
}



for (int i = 1; i <= dzielenie; i++) //  
{
        odejmowanie *= i;
}

int wynik = suma / (temp - odejmowanie);
return wynik;
}
