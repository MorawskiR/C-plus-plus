#include <iostream>
#include <string>
#include <vector>
#include <array>
using namespace std;
struct pudlo
{
    float wysokosc;
};
void funkcja(pudlo p1);
void show(const pudlo * pda);
int main() {
pudlo czosnek { 30.55 };
funkcja(czosnek);
show(&czosnek);
}
void funkcja(pudlo p1)
{
    cout<<p1.wysokosc;
}
void show(const pudlo * pda)
{
    cout<<pda->wysokosc;
}
