#include <iostream>
#include "event.h"
using namespace std;

class Zeszyt
{
public :
 int kartki;
 string kolor;
 string srodek;
public:
    Zeszyt(int n = 100, string k ="zolty",string s  ="kratka"){
        kartki = n;
        kolor = k;
        srodek = s;
    }
};

int main()
{
  Zeszyt z1;
  cout<<z1.kartki<<" stron"<<endl;
  cout<<z1.srodek<<" w srodku " <<endl;
  cout<<z1.kolor <<" kolor okladki"<<endl;
  return 0;
}
